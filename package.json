{
  "name": "parket-crm",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "steiger": "steiger",
    "prepare": "husky install",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed-test": "tsx prisma/seed-test.ts",
    "db:seed-dev": "tsx prisma/seed-dev.ts",
    "db:seed-all": "npm run db:seed && npm run db:seed-test",
    "db:reset": "prisma migrate reset --force",
    "dev:setup": "node scripts/dev-setup.js",
    "dev:clean": "docker-compose down -v && docker volume prune -f",
    "db:studio": "prisma studio",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:ci && npm run test:e2e",
    "test:setup": "playwright install",
    "config:check": "node scripts/check-config.js",
    "config:validate": "npm run config:check && npm run type-check && npm run lint",
    "env:check": "node scripts/check-production-env.js",
    "env:validate": "npm run env:check",
    "test:email": "node scripts/test-email-mailhog.cjs",
    "minio:setup": "node scripts/setup-minio.js",
    "minio:start": "docker-compose up -d minio",
    "minio:stop": "docker-compose stop minio",
    "minio:logs": "docker-compose logs -f minio",
    "migration:test-mysql": "tsx scripts/migration/test-mysql-connection.ts",
    "migration:run": "tsx scripts/migration/migrate.ts",
    "migration:dry-run": "tsx scripts/migration/migrate.ts --dry-run",
    "migration:skip-images": "tsx scripts/migration/migrate.ts --skip-images"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@aws-sdk/client-cloudfront": "^3.896.0",
    "@aws-sdk/client-s3": "^3.896.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.16.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.15",
    "@sentry/nextjs": "^10.15.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.2",
    "chalk": "^5.6.2",
    "class-variance-authority": "^0.7.1",
    "cli-progress": "^3.12.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "lucide-react": "^0.544.0",
    "mysql2": "^3.15.3",
    "next": "15.5.4",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.68.0",
    "sharp": "^0.34.5",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^10.0.0",
    "@feature-sliced/steiger-plugin": "^0.5.7",
    "@playwright/test": "^1.55.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^15.2.10",
    "msw": "^2.11.3",
    "next-intl": "^4.3.9",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "prisma": "^6.16.3",
    "steiger": "^0.5.11",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
